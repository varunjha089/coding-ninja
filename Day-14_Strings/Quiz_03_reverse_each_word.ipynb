   "source": [
    "def reverseEachWord(string) :\n",
    "    # Your code goes here\n",
    "    n = string.split(\" \")\n",
    "    s = \"\"\n",
    "    for i in range(len(n)):\n",
    "        s += n[i][::-1]\n",
    "        if i != len(n) - 1:\n",
    "            s += \" \"\n",
    "    print(s)"
   ]
