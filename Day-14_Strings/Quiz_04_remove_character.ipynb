   "source": [
    "def removeAllOccurrencesOfChar(string, ch) :\n",
    "    # Your code goes here\n",
    "    s = \"\"\n",
    "    for i in string:\n",
    "        if i != ch:\n",
    "            s += i\n",
    "            \n",
    "    return s"
   ]
  },
